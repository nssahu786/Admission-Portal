<body>


  <section class="vh-120 bg-image" style="background-image: url(/assets/img/img4.jpg);background-position: center;background-size: cover;height: 850px;">
    <div class="mask d-flex align-items-center h-100 gradient-custom-3">
      <div class="container h-100">
        <div class="row d-flex justify-content-center align-items-center h-100">
          <div class="col-md-6">
            <img src="assets/img/fo1.jpeg" alt="" class="w-100">
          </div>
          <div class="col-md-6">
            <div class="card" style="border-radius: 15px;">
              <div class="card-body p-5">
                <h2 class="text-uppercase text-center mb-5 text-success">Create an account</h2>
                <%if(message.length > 0){ %>
                  <div class="alert alert-danger">
                      <%=message %>
                  </div>
                <% }%>
                <form action ="/registerinsert" method="post">
                <div class="form-outline mb-4">
                  <input type="text" name="name" id="form3Example1cg" class="form-control form-control-lg" />
                  <label class="form-label text-danger" for="form3Example1cg">Your Name</label>
                </div>
                <div class="form-outline mb-4">
                  <input type="email" name="email" id="form3Example3cg" class="form-control form-control-lg" />
                  <label class="form-label text-danger" for="form3Example3cg">Your Email</label>
                </div>
                <div class="form-outline mb-4">
                  <input type="password" name="password" id="form3Example4cg" class="form-control form-control-lg" />
                  <label class="form-label text-danger" for="form3Example4cg">Password</label>
                </div>
                <div class="form-outline mb-4">
                  <input type="password" name="confirmpassword" id="form3Example4cdg" class="form-control form-control-lg" />
                  <label class="form-label text-danger" for="form3Example4cdg">Repeat your password</label>
                </div>
                <div class="d-flex justify-content-center">
                  <button type="submit" class="btn btn-success btn-block btn-lg gradient-custom-4 text-body">REGISTER</button>
                </div>
                <p class="text-center text-muted mt-5 mb-0">Have already an account? <a href="/"
                    class="fw-bold text-body-warning"><u>LOGIN HERE</u></a></p>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
</section>
   






    
