<%- include('./header.ejs') %>
    <br><br>
    <section class="cont">
    </section>
    <br><br>

<h1 class="text-center text-success"><span class="text-primary">CONTACT</span> US</h1><br><br>
<section class="mb-4">
    <p class="w-responsive mx-auto mb-5 para2">Do you have any questions? Please do not hesitate to contact us directly. Our team will come back to you within a matter of hours to help you.</p><br><br>
    <div class="container">
        <div class="row">
            <div class="col-md-1"></div>
            <div class="col-md-7 mb-md-0 mb-5">
                <form action="/user/contactinsert" method="post">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="md-form mb-0">
                                <input type="text" id="name" name="name" class="form-control">
                                <label for="name" class="temp1">Your name</label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="md-form mb-0">
                                <input type="text" id="email" name="email" class="form-control">
                                <label for="email" class="temp1">Your email</label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="md-form mb-0">
                                <input type="text" id="subject" name="subject" class="form-control">
                                <label for="subject" class="temp1">Subject</label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="md-form">
                                <textarea type="text" id="message" name="message" rows="2" class="form-control md-textarea"></textarea>
                                <label for="message" class="temp1">Your message</label>
                            </div>
                        </div>
                    </div><br>
                    <button type="submit" class="btn btn-success btn-lg">SEND MESSAGE...</button>
                </form>
                <div class="status"></div>
            </div>
            <div class="col-md-3 text-center">
                <ul class="list-unstyled mb-0">
                    <li><i class="fas fa-map-marker-alt fa-2x"></i>
                        <p class="para3">234/80 - UFG, New Street,Gwalior India</p>
                    </li>
                    <li><i class="fas fa-phone mt-4 fa-2x"></i>
                        <p class="para3">+91 7869391234</p>
                    </li>
                    <li><i class="fas fa-envelope mt-4 fa-2x"></i>
                        <p class="para3">contact@nssinfosys.com</p>
                    </li>
                </ul>
            </div>
            <div class="col-md-1"></div>
        </div>
    </div>
</section>



<%- include('./footer.ejs') %>