<%- include('../header.ejs') %>
<br><br><br>
<div class="banner12">
    <br><br>
    <h1 class="text-center bg-light welcome"><span style="text-transform: uppercase;color: lawngreen;position: relative;top: 111px;"><b>STUDENT DASHBOARD</b></span></h1>
</div>
<div class="banner8">
    <br><br> <h1 class="text-center bg-light text-warning welcome">DISPLAY DETAILS</h1>
</div><br>

<section>
  <div class="container">
    <div class="row">
      <div class="col-md-1"></div>
      <div class="col-md-10">
        <table class="styled-table">
          <thead>
              <tr>
                <th>S NO.</th>
                <th>NAME</th>
                <th>EMAIL</th>
                <th>MOBILE</th>
                <th>ADDRESS</th>
                <th>GENDER</th>
                <th>COURSE</th>
                <th>BRANCH</th>
                <th>COLLEGE</th>
              </tr>
          </thead>
          <tbody>
            <% var i=1 %>    
            <tr class="table-primary">
                <td><%= i++ %></td>
                <td><%= data.name %></td>
                <td><%= data.email %></td>
                <td><%= data.mobile %></td>
                <td><%= data.address %></td>
                <td><%= data.gender %></td>
                <td><%= data.course %></td>
                <td><%= data.branch %></td>
                <td><%= data.college %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="col-md-1"></div>
    </div>
  </div>
</section>
  
  
<%- include('../footer.ejs') %>